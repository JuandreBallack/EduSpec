    EduSpecHtmlHelper.Label(Html, "Billing Cycle:", "editorLabel");
    EduSpecHtmlHelper.EndAndNewColumn(Html, "edit_form", 120);// Second column
    EduSpecHtmlHelper.ComboBox(Html, "formeditor", Model, "BillingCycleTypeID", 220, "Get_BillingCycleType_PickList", null);
    EduSpecHtmlHelper.EndColumn(Html);
    EduSpecHtmlHelper.EndRow(Html);

    EduSpecHtmlHelper.NewRow(Html);
    EduSpecHtmlHelper.NewColumn(Html, "edit_form", 120);// First column
    EduSpecHtmlHelper.Label(Html, "Age Import Type:", "editorLabel");
    EduSpecHtmlHelper.EndAndNewColumn(Html, "edit_form", 120);// Second column
    EduSpecHtmlHelper.ComboBox(Html, "formeditor", Model, "AgeImportTypeID", 220, "Get_AgeAnalysisImportType_PickList", null);
    EduSpecHtmlHelper.EndColumn(Html);
