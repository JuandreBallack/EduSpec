@model DevExpress.XtraReports.UI.XtraReport

@Html.DevExpress().WebDocumentViewer(settings => {
    settings.Name = "wdv" + ViewData["Name"];
    settings.Width = 1100;
    settings.Height = 550;
    settings.ClientSideEvents.Init = "function(s, e) {s.previewModel.reportPreview.zoom(1);}";
}).Bind(Model).GetHtml()