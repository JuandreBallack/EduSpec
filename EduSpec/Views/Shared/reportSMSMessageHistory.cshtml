@model DevExpress.XtraReports.UI.XtraReport

@Html.DevExpress().PopupControl(settings =>
{
    PopupUtils.SetPopupViewProperties(settings, "pcPrintSMSMessageHistoryReport", "SMS Message history");
    settings.CallbackRouteValues = new {Controller = "Shared", Action = "reportSMSMessageHistory" };
    settings.ClientSideEvents.BeginCallback = "function(s, e) { ; }";
    settings.ClientSideEvents.EndCallback = "function(s, e) {  LoadingPanel.Hide(); s.Show(); }";
    settings.SetContent(() =>
    {
        Html.RenderPartial("DocumentViewer", Model, new ViewDataDictionary { { "Name", "SMSMessageHistory" } });
    });
}).GetHtml()
